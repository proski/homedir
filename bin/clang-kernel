#! /bin/sh

CLANG=clang
REMOVE_ARGS="-maccumulate-outgoing-args -falign-jumps=1 -falign-loops=1"

old_args="$@"
new_args="$CLANG"
for arg in $old_args; do
	keep=1
	for rarg in $REMOVE_ARGS; do
		if test "$arg" = "$rarg"; then
			keep=
			break
		fi
	done
	if test -n "$keep"; then
		new_args="$new_args $arg"
	fi
done

$new_args
